//-------------------------------------------------
// bewegend luchtdoel geschut
//-------------------------------------------------
// accum 0 = actief (1=ja, 0=nee)
// script_mover
luchtdoel0_tagger
{
	spawn
	{
		wait 50
		trigger self setactive
		trigger self linksom
	}
	trigger linksom
	{
		trigger self sound_start
		wait 700
		trigger self sound_loop
		faceangles 0 0 0 4000
		faceangles 0 90 0 4000
		trigger self sound_end
		wait 770
		trigger self rechtsom
	}
	trigger rechtsom
	{
		trigger self sound_start
		wait 700
		trigger self sound_loop
		faceangles 0 0 0 4000
		faceangles 0 -90 0 4000
		trigger self sound_end
		wait 770
		trigger self linksom
	}

	trigger setinactive
	{
		accum 0 set 0
	}
	trigger setactive
	{
		accum 0 set 1
	}

	trigger sound_start
	{
		stopsound
		playsound sound/movers/motors/motor_start_01.wav volume 127
	}
	trigger sound_loop
	{
		stopsound
		playsound sound/movers/motors/motor_loop_01.wav looping volume 127
	}
	trigger sound_end
	{
		stopsound
		playsound sound/movers/motors/motor_end_01.wav volume 127
	}
	trigger sound_shoot
	{
		stopsound
		playsound sound/weapons/artillery/artillery_expl_2.wav volume 512
	}

	trigger shoot0
	{
		accum 0 abort_if_equal 0
		resetscript
		halt
		faceangles 0 74 0 500

		trigger self sound_shoot
		setstate luchtdoel0_flash default
		wait 400
		setstate luchtdoel0_flash invisible
		wait 1000
		alertentity luchtdoel_knal0
		wait 2500

		trigger self linksom
	}
	trigger shoot1
	{
		accum 0 abort_if_equal 0
		resetscript
		halt
		faceangles 0 16 0 500

		trigger self sound_shoot
		setstate luchtdoel0_flash default
		wait 400
		setstate luchtdoel0_flash invisible
		wait 1000
		alertentity luchtdoel_knal4
		wait 2500

		trigger self rechtsom
	}
	trigger shoot2
	{
		accum 0 abort_if_equal 0
		resetscript
		halt
		faceangles 0 -41 0 500

		trigger self sound_shoot
		setstate luchtdoel0_flash default
		wait 400
		setstate luchtdoel0_flash invisible
		wait 1000
		alertentity luchtdoel_knal8
		wait 2500

		trigger self rechtsom
	}
}

