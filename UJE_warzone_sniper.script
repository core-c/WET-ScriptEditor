	}
	trigger timeout
	{
		accum 2 abort_if_equal 0	// timeout al verlopen..
		accum 2 inc -1
		accum 2 abort_if_greater_than 0
		trigger self endsound
	}

	trigger play
	{
		accum 0 abort_if_equal 1
		resetscript
		trigger self reset
		setrotation 0 -78 0		// plaatje draaien
		stopsound			// geluid aan
		playsound sound/uje_warzone_sniper/house1.wav looping volume 1024
		accum 0 set 1

		trigger draaitafel1 stop //endsound
		trigger draaitafel2 stop //endsound
	}
	trigger stop
	{
		accum 0 abort_if_equal 0
		halt
		stoprotation
		stopsound
		accum 0 set 0
	}
	trigger endsound
	{
		accum 0 abort_if_equal 0
		// geluid langzaam uit..
