// script_mover
// accum 0: status (0=opened, 1=closed, 2=moving)
slagboom0
{
	spawn
	{
		wait 50
		accum 0 set 1
	}

	trigger open
	{
		accum 0 abort_if_equal 0
		accum 0 abort_if_equal 2
		accum 0 set 2
		faceangles -90 0 0 1200 deccel
		accum 0 set 0

		trigger luchtdoel0_tagger setinactive
	}
	trigger close
	{
		accum 0 abort_if_equal 1
		accum 0 abort_if_equal 2
		accum 0 set 2
		faceangles 0 0 0 1200 deccel
		accum 0 set 1

		trigger luchtdoel0_tagger setactive
	}

	trigger toggle
	{
		trigger self open
		trigger self close
	}
}

