uje_bommer0
{
	spawn
	{
		wait 50
		setstate uje_bommer0 invisible
		wait 3000
		trigger self movestart
	}

	trigger movestart
	{
		accum 0 set -1

		// bommer0 op pad0 plaatsen
//!!		setstate uje_bommer0 invisible
//		followspline 0 bommer0_pad0 10000 length -64
		setposition bommer0_pad0
		faceangles 0 270 0 1
		setstate uje_bommer0 default

		// geluid starten van vliegtuig-aanval
		alertentity aanval

		// kraters "dichten"
		trigger bom0_krater0 fix
		trigger bom0_krater1 fix
		trigger bom0_krater2 fix
		trigger bom0_krater3 fix
		trigger self movenext
	}
	trigger movenext
	{
		accum 0 inc 1
		trigger self move
	}
	trigger move
	{
		accum 0 trigger_if_equal 0 uje_bommer0 move_0
		accum 0 trigger_if_equal 1 uje_bommer0 move_1
