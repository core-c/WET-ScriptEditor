	{
		accum 0 abort_if_not_equal 1
		accum 0 set 2
		faceangles 0 0 0 1000 deccel
		accum 0 set 0

		wait 2100				// wacht tot de trigger-timeout is verlopen
		trigger self continue
		trigger tele_trigger_axis show
	}

	trigger continue
	{
		// Er moet wel een axis in staan, (bit 2 = axis in rode telefooncel)
		// er mag geen allies in staan. (bit 3 = allies in rode telefooncel)
		globalaccum 5 abort_if_not_bitset 2
