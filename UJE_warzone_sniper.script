//func_invisible_users
// accum 0 = een rol draait (0=nee, 1=ja)
// accum 1 = score (niet in gebruik)
// accum 2 = score (100-tallen)
// accum 3 = score (10-tallen)
// accum 4 = score (eenheden)
// accum 5 = winst score
// globalaccum 1 = tmp register (tbv. if-then-else)
// globalaccum 2 = vasthouden rol teller mechanisme
// globalaccum 3 = kop/munt gokken
// globalaccum 4 = kop/munt winst-score
gok_knop
{
	spawn
	{
		wait 100
		remapshader "textures/UJE_warzone_sniper/uje_gamble" "textures/UJE_warzone_sniper/uje_gamble_light"
		remapshaderflush

		trigger self win_off

		// niet bezig met gokken
		accum 0 set 0

		// begin met 500 punten
		accum 1 set 500
		accum 2 set 5
		accum 3 set 0
		accum 4 set 0
		trigger self display_score
		trigger score_display show_gain_0

		accum 5 set 0

		// vasthouden rollen mag nu
		globalaccum 2 set 0
		// niet bezig met gokken kop/munt
		globalaccum 3 set 0
		// de huidige kop/munt winst-score
		globalaccum 4 set 0

		setstate gok_bet invisible
	}
	activate
	{
		// bezig met gokken?? dan niet triggeren..
		accum 0 abort_if_not_equal 0

		trigger self start_gokken
	}
	trigger start_gokken
	{
		// bezig met gokken
		accum 0 set 1

		// geluidje
		enablespeaker uje_press_sound

		// 1 punt eraf
		trigger self score_sub1
		trigger self display_score
		trigger score_display show_gain_0

		setstate gok_knop invisible
		setstate gok_hold_1 invisible
		setstate gok_hold_2 invisible
		setstate gok_hold_3 invisible
		setstate gok_herstel invisible

		remapshader "textures/UJE_warzone_sniper/uje_gamble" "textures/UJE_warzone_sniper/uje_gamble"
		remapshaderflush

		trigger self win_off

		trigger gok_rol_1 start
		trigger gok_rol_2 start
		trigger gok_rol_3 start
		wait random 900 2300
		wait 100
		trigger gok_rol_1 stop
		wait random 900 1500
		wait 100
		trigger gok_rol_2 stop
		wait random 1300 2000
		wait 100
		trigger gok_rol_3 stop

		wait 50	// wacht tot de rol stilstaat
		trigger gok_rol_1 stershader
		trigger gok_rol_2 stershader
		trigger gok_rol_3 stershader

		trigger self check_after_hold
		trigger self check_hold
		trigger self check_hold_allowed

		// vasthouden rollen
		trigger gok_rol_1 hold_off
		trigger gok_rol_2 hold_off
		trigger gok_rol_3 hold_off

		remapshader "textures/UJE_warzone_sniper/uje_gamble" "textures/UJE_warzone_sniper/uje_gamble_light"
		remapshader "textures/UJE_warzone_sniper/uje_cancel" "textures/UJE_warzone_sniper/uje_cancel"
		remapshader "textures/UJE_warzone_sniper/uje_hold1" "textures/UJE_warzone_sniper/uje_hold1"
		remapshader "textures/UJE_warzone_sniper/uje_hold2" "textures/UJE_warzone_sniper/uje_hold2"
		remapshader "textures/UJE_warzone_sniper/uje_hold3" "textures/UJE_warzone_sniper/uje_hold3"
		remapshaderflush

		trigger self check_score_200
		trigger self check_score_100
		trigger self check_score_40
		trigger self check_score_20
		trigger self check_score_20_star
		trigger self check_score_16_0
		trigger self check_score_16_1
		trigger self check_score_16_2
		trigger self check_score_16_3
		trigger self check_score_12_0
		trigger self check_score_12_1
		trigger self check_score_12_2
		trigger self check_score_12_3
		trigger self check_score_8_0
		trigger self check_score_8_1
		trigger self check_score_4
		trigger self check_score_2

		trigger self continue
	}
	trigger continue
	{
		setstate gok_knop default
		// niet bezig met gokken
		accum 0 set 0
	}

	// 3x bar7 gescored?
	trigger check_score_200
	{
		trigger gok_rol_1 check_score_200
		globalaccum 1 abort_if_not_equal 1

		trigger gok_rol_2 check_score_200
		globalaccum 1 abort_if_not_equal 1

		trigger gok_rol_3 check_score_200
		globalaccum 1 abort_if_not_equal 1

		resetscript

