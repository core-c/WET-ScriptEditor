	{
		accum 0 abort_if_not_equal 0
		accum 0 set 2
		trigger tele_trigger_axis hide
		faceangles 0 -90 0 1000 deccel
		accum 0 set 1

		wait 2000				// na 2 seconden vanzelf de deur sluiten
		trigger self close
	}

