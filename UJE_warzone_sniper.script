//-- vloer
// tbv. target_script_trigger
secret_script
{
	trigger timeout
	{
		trigger trigger_high_secret timeout
		trigger trigger_low_secret timeout
	}
}

//trigger_multiple
// accum 0 = timeout waarde (0=geen speler in trigger, <>0=speler in trigger)
trigger_high_secret
{
	spawn
	{
		wait 50
		accum 0 set 0
	}
	activate
	{
		accum 0 set 10
		trigger clip_secret show
		trigger trigger_low_secret high_active
	}

	trigger timeout
	{
		accum 0 abort_if_equal 0
		accum 0 inc -1
		accum 0 abort_if_greater_than 0
		trigger trigger_low_secret high_inactive
	}
}

//trigger_multiple
// accum 0 = timeout waarde (0=geen speler in trigger, <>0=speler in trigger)
// accum 1 = upper trigger activated?? (0=nee, 1=ja)
trigger_low_secret
{
	spawn
	{
		wait 50
		accum 0 set 0
		accum 1 set 0
	}
	activate
	{
		accum 0 set 2
		trigger self check_passage
	}

	trigger high_active
	{
		accum 1 set 1
	}
	trigger high_inactive
	{
		accum 1 set 0
	}

	trigger timeout
	{
		accum 0 abort_if_equal 0
		accum 0 inc -1
		accum 0 abort_if_greater_than 0
	}

	trigger check_passage
	{
		accum 1 abort_if_equal 1
		trigger clip_secret hide
		wait 1000
		trigger clip_secret show
	}
}

//func_static
// accum 0 = zichtbaar?? (0=nee, 1=ja)
clip_secret
{
	spawn
	{
		wait 50
		accum 0 set 1
	}

	trigger show
	{
		accum 0 abort_if_equal 1
		accum 0 set 1
		setstate clip_secret default
	}
	trigger hide
	{
		accum 0 abort_if_equal 0
		accum 0 set 0
		setstate clip_secret invisible
	}
}

