// [UJE]C
// All comments in this script exist for my own convenience..

//--- gokkast
// globalaccum 1 = tmp register (tbv. if-then-else)
// globalaccum 2 = gokkast rol vasthouden teller mechanisme
// globalaccum 3 = kop/munt gokken
// globalaccum 4 = kop/munt winst-score
//--- teleports
// globalaccum 5: bit 0 = paradrop allies is bezet
//              : bit 1 = paradrop axis is bezet
//              : bit 2 = axis in rode telefooncel
//              : bit 3 = allies in rode telefooncel
//              : bit 4 = axis in blauwe telefooncel
//              : bit 5 = allies in blauwe telefooncel

game_manager
{
	spawn
	{
		wait 50

		globalaccum 5 bitreset 0		// paradrop allies vrij
		globalaccum 5 bitreset 1		// paradrop axis vrij
		globalaccum 5 bitreset 2		// geen axis in rode telefooncel
		globalaccum 5 bitreset 3		// geen allies in rode telefooncel
		globalaccum 5 bitreset 4		// geen axis in blauwe telefooncel
		globalaccum 5 bitreset 5		// geen allies in blauwe telefooncel

		// tijden
		wm_axis_respawntime	3
		wm_allied_respawntime	3
		wm_set_round_timelimit	30

		// Objectives
		wm_number_of_objectives 8
		// 1: snipe
		// 2: snipe
		// 3: snipe
		// 4: snipe
		// 5: snipe
		// 6: snipe
		// 7: snipe
		// 8: snipe

		// Objective status indicators
		//wm_objective_status <objective> <team (0=Axis, 1=Allies)> <status (0=neutral 1=complete 2=failed)>
		wm_objective_status 1 1 0
		wm_objective_status 1 0 0
		wm_objective_status 2 1 0
		wm_objective_status 2 0 0
		wm_objective_status 3 1 0
		wm_objective_status 3 0 0
		wm_objective_status 4 1 0
		wm_objective_status 4 0 0
		wm_objective_status 5 1 0
		wm_objective_status 5 0 0
		wm_objective_status 6 1 0
		wm_objective_status 6 0 0
		wm_objective_status 7 1 0
		wm_objective_status 7 0 0
		wm_objective_status 8 1 0
		wm_objective_status 8 0 0

		// Stopwatch verdedigend team
		wm_set_defending_team 0

		// Winnaars als de rondetijd om is (0=Axis, 1=Allies, -1=gelijkspel)
		wm_setwinner 1
	}

	trigger timelimit_hit
	{
		// maak er een gelijkspel van
		wm_setwinner -1

		// nog een laatste rondje laten vliegen..
		trigger uje_bommer0 movestart

		wm_endround
	}

	// uje_bommer0 eerste bombardement
	trigger bommen0_1e
	{
		// de 1e bom
		alertentity bom0_0
		wait 200
		// krater laten zien
		trigger bom0_krater0 destroy
		// explosie effecten
		alertentity bom0_0_effect
		alertentity bom0_0_explosion

		// beving
		alertentity bom0_01_rumble
		wait 800

		// de 2e bom
		alertentity bom0_1
		wait 200
		trigger bom0_krater1 destroy
		alertentity bom0_1_effect
		alertentity bom0_1_explosion
	}

	// uje_bommer0 tweede bombardement
	trigger bommen0_2e
	{
		// de 3e bom
		alertentity bom0_2
		wait 200
		// krater laten zien
		trigger bom0_krater2 destroy
		// explosie effecten
		alertentity bom0_2_effect
		alertentity bom0_2_explosion

		// beving
		alertentity bom0_23_rumble
		wait 800

		// de 4e bom
		alertentity bom0_3
		wait 200
		trigger bom0_krater3 destroy
		alertentity bom0_3_effect
		alertentity bom0_3_explosion
	}

	// uje_bommer1 tapijtje
	trigger bommen1
	{
		wait 3000			// scene timing
		// de 1e bom
		alertentity bom1_0
		wait 750
		alertentity bom1_1
		wait 750
		alertentity bom1_2
		wait 750
		alertentity bom1_3
		wait 750
		alertentity bom1_4
		wait 750
		alertentity bom1_5
		wait 750
		alertentity bom1_6
	}
}


