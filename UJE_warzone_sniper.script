	trigger start_betting
	{
		setstate gok_bet default
		setstate gok_knop invisible
		setstate gok_hold_1 invisible
		setstate gok_hold_2 invisible
		setstate gok_hold_3 invisible
		setstate gok_herstel default

		remapshader "textures/UJE_warzone_sniper/uje_cancel" "textures/UJE_warzone_sniper/uje_cancel_light"
		remapshader "textures/UJE_warzone_sniper/uje_gamble" "textures/UJE_warzone_sniper/uje_gamble"
		remapshaderflush

		enablespeaker uje_bet_sound		// looped
		trigger self start_bet
	}

	trigger start_bet
	{
		remapshader "textures/UJE_warzone_sniper/uje_bet" "textures/UJE_warzone_sniper/uje_bet_light"
		remapshaderflush
		wait 200
		remapshader "textures/UJE_warzone_sniper/uje_bet" "textures/UJE_warzone_sniper/uje_bet"
		remapshaderflush
		wait 200
		trigger self start_bet
	}
	trigger stop_bet
	{
		setstate gok_bet invisible
		setstate gok_herstel invisible

		remapshader "textures/UJE_warzone_sniper/uje_cancel" "textures/UJE_warzone_sniper/uje_cancel"
		remapshader "textures/UJE_warzone_sniper/uje_bet" "textures/UJE_warzone_sniper/uje_bet"
		remapshader "textures/UJE_warzone_sniper/uje_gamble" "textures/UJE_warzone_sniper/uje_gamble_light"
		remapshaderflush

		disablespeaker uje_bet_sound		// looped

		trigger self collect_score
		globalaccum 3 set 0
		globalaccum 4 set 0
		trigger self continue
	}

	trigger bet
	{
		resetscript

		stopsound
		disablespeaker uje_bet_sound		// looped

		accum 5 random 100
		globalaccum 1 set 0
		trigger self check_bet
		globalaccum 1 abort_if_equal 1
		// winst-score verloren..

		// geluidje punten verloren
		enablespeaker uje_betlost_sound
		wait 1190
		disablespeaker uje_betlost_sound

		globalaccum 4 set 0

		trigger self stop_bet
	}
	trigger check_bet
	{
		accum 5 abort_if_less_than 50
		globalaccum 1 set 1
		// winst verdubbeld..
		resetscript

		enablespeaker uje_doubled_sound
		wait 370
		disablespeaker uje_doubled_sound

		trigger self double_bet
		globalaccum 3 abort_if_equal 0
		trigger self start_betting
	}


	trigger double_bet
	{
		globalaccum 3 abort_if_equal 0
		globalaccum 4 abort_if_equal 0

		globalaccum 4 trigger_if_equal 2 gok_knop bet_raiseto_4
		globalaccum 4 trigger_if_equal 4 gok_knop bet_raiseto_8
		globalaccum 4 trigger_if_equal 8 gok_knop bet_raiseto_16
		globalaccum 4 trigger_if_equal 16 gok_knop bet_raiseto_32
		globalaccum 4 trigger_if_equal 32 gok_knop bet_raiseto_64
		globalaccum 4 trigger_if_equal 64 gok_knop bet_raiseto_128
		globalaccum 4 trigger_if_equal 128 gok_knop bet_raiseto_200

		//trigger self continue
		trigger self start_betting
	}
